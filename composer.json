{
    "name": "jobbrander/moonclerk-php",
    "type": "library",
    "description": "Making it easy to integrate with Moonclerk's read only API.",
    "keywords": [
        "moonclerk",
        "api client",
        "checkout",
        "payments",
        "recurring payments"
    ],
    "homepage": "https://github.com/JobBrander/moonclerk-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Karl Hughes",
            "email": "khughes.me@gmail.com",
            "homepage": "http://www.karllhughes.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/php-code-coverage": "~2.0",
        "mockery/mockery": ">=0.9.4"
    },
    "autoload": {
        "psr-4": {
            "JobBrander\\Moonclerk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JobBrander\\Moonclerk\\Test\\": "test/src/"
        }
    }
}
